{
  "compilerOptions": {
    "jsx": "react-native",
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noEmit": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "react": [
        "node_modules/react"
      ],
      "invariant": [
        "node_modules/invariant"
      ]
    }
  },
  "include": [
    "**/*"
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "extends": "expo/tsconfig.base"
}
